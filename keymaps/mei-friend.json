{
  ".platform-darwin atom-workspace": {
    "alt-cmd-m": "mei-friend:toggle",
    "alt-cmd-c": "mei-friend:convertToMEI"
  },
  ".platform-win32 atom-workspace, .platform-linux atom-workspace": {
    "ctrl-shift-m": "mei-friend:toggle",
    "ctrl-shift-c": "mei-friend:convertToMEI"
  },
  ".mei-friend": {
    "?": "mei-friend:toggleHelp",
    "escape": "mei-friend:hideHelp",
    "s": "mei-friend:addSlur",
    "ctrl-s": "mei-friend:addSlurBelow",
    "t": "mei-friend:addTie",
    "ctrl-t": "mei-friend:addTieBelow",
    "h": "mei-friend:addCresHairpin",
    "shift-h": "mei-friend:addDimHairpin",
    "ctrl-h": "mei-friend:addCresHairpinBelow",
    "ctrl-shift-h": "mei-friend:addDimHairpinBelow",
    "f": "mei-friend:addFermata",
    "ctrl-f": "mei-friend:addFermataBelow",
    "i": "mei-friend:addDirective",
    "ctrl-i": "mei-friend:addDirectiveBelow",
    "d": "mei-friend:addDynamics",
    "ctrl-d": "mei-friend:addDnamicsBelow",
    "shift-t": "mei-friend:addTempo",
    "alt-shift-t": "mei-friend:addTempoTstamp",
    "a": "mei-friend:addArpeggio",
    "g": "mei-friend:addGlissando",
    "p": "mei-friend:addPedalDown",
    "ctrl-p": "mei-friend:addPedalUp",
    "m": "mei-friend:addMordentAbove",
    "ctrl-m": "mei-friend:addMordentBelow",
    "shift-m": "mei-friend:addMordentAboveUpper",
    "shift-ctrl-m": "mei-friend:addMordentBelowUpper",
    "l": "mei-friend:addTrillAbove",
    "ctrl-l": "mei-friend:addTrillBelow",
    "n": "mei-friend:addTurnAbove",
    "ctrl-n": "mei-friend:addTurnBelow",
    "shift-n": "mei-friend:addTurnAboveLower",
    "shift-ctrl-n": "mei-friend:addTurnBelowLower",
    "shift-s": "mei-friend:toggleStacc",
    "shift-v": "mei-friend:toggleAccent",
    "shift-n": "mei-friend:toggleTenuto",
    "shift-o": "mei-friend:toggleMarcato",
    "shift-i": "mei-friend:toggleStacciss",

    "b": "mei-friend:addBeam",
    "o": "mei-friend:addOctave8Above",
    "ctrl-o": "mei-friend:addOctave8Below",
    "alt-o": "mei-friend:addOctave15Above",
    "alt-ctrl-o": "mei-friend:addOctave15Below",

    "q": "mei-friend:nightMode",
    "x": "mei-friend:invertPlacement",
    "ctrl-x": "mei-friend:betweenPlacement",
    "right": "mei-friend:nextNote",
    "left": "mei-friend:previousNote",
    "up": "mei-friend:layerUp",
    "down": "mei-friend:layerDown",
    "shift-up": "mei-friend:shiftPitchNameUp",
    "shift-down": "mei-friend:shiftPitchNameDown",
    "delete": "mei-friend:delete",
    "backspace": "mei-friend:delete",
    "shift-c": "mei-friend:cleanAccid",
    "shift-r": "mei-friend:renumberMeasuresTest",
    "ctrl-shift-r": "mei-friend:renumberMeasures"
  },
  ".platform-darwin .mei-friend": {
    "cmd-right": "mei-friend:nextMeasure",
    "cmd-left": "mei-friend:previousMeasure",
    "ctrl-cmd-right": "mei-friend:nextPage",
    "ctrl-cmd-left": "mei-friend:previousPage",
    "cmd-shift-up": "mei-friend:shiftOctaveUp",
    "cmd-shift-down": "mei-friend:shiftOctaveDown",
    "cmd-alt-up": "mei-friend:moveElementStaffUp",
    "cmd-alt-down": "mei-friend:moveElementStaffDown"
  },
  ".platform-win32 .mei-friend, .platform-linux .mei-friend": {
    "ctrl-right": "mei-friend:nextMeasure",
    "ctrl-left": "mei-friend:previousMeasure",
    "ctrl-alt-right": "mei-friend:nextPage",
    "ctrl-alt-left": "mei-friend:previousPage",
    "ctrl-shift-up": "mei-friend:shiftOctaveUp",
    "ctrl-shift-down": "mei-friend:shiftOctaveDown",
    "ctrl-alt-up": "mei-friend:moveElementStaffUp",
    "ctrl-alt-down": "mei-friend:moveElementStaffDown"
  }
}
